iv>\n      </div>\n    </transition>\n  </nav>\n</template>\n\n<script>\nimport { mapGetters, mapState } from 'vuex'\n\nexport default {\n  name: 'AppNavbar',\n  data() {\n    return {\n      dropdownOpen: false,\n      mobileMenuOpen: false\n    }\n  },\n  computed: {\n    ...mapGetters(['isAuthenticated', 'isUser', 'isOwner', 'isAdmin']),\n    ...mapState({\n      user: state => state.user\n    })\n  },\n  methods: {\n    toggleDropdown() {\n      this.dropdownOpen = !this.dropdownOpen\n    },\n    toggleMobileMenu() {\n      this.mobileMenuOpen = !this.mobileMenuOpen\n    },\n    logout() {\n      this.dropdownOpen = false\n      this.mobileMenuOpen = false\n      this.$store.dispatch('logout')\n      this.$router.push('/')\n    },\n    hideMenus() {\n      this.dropdownOpen = false\n      this.mobileMenuOpen = false\n    }\n  },\n  watch: {\n    '$route'() {\n      this.hideMenus()\n    }\n  },\n  mounted() {\n    document.addEventListener('click', (e) => {\n      const target = e.target\n      if (!this.$el.contains(target)) {\n        this.dropdownOpen = false\n      }\n    })\n  }\n}\n</script>"],"mappings":"AA8GA,SAAAA,UAAA,EAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,YAAA;MACAC,cAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAN,UAAA;IACA,GAAAC,QAAA;MACAM,IAAA,EAAAC,KAAA,IAAAA,KAAA,CAAAD;IACA;EACA;EACAE,OAAA;IACAC,eAAA;MACA,KAAAN,YAAA,SAAAA,YAAA;IACA;IACAO,iBAAA;MACA,KAAAN,cAAA,SAAAA,cAAA;IACA;IACAO,OAAA;MACA,KAAAR,YAAA;MACA,KAAAC,cAAA;MACA,KAAAQ,MAAA,CAAAC,QAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,UAAA;MACA,KAAAb,YAAA;MACA,KAAAC,cAAA;IACA;EACA;EACAa,KAAA;IACA,QAAAC,CAAA;MACA,KAAAF,SAAA;IACA;EACA;EACAG,QAAA;IACAC,QAAA,CAAAC,gBAAA,UAAAC,CAAA;MACA,MAAAC,MAAA,GAAAD,CAAA,CAAAC,MAAA;MACA,UAAAC,GAAA,CAAAC,QAAA,CAAAF,MAAA;QACA,KAAApB,YAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}